<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.member.mapper.KindsMapper">

<select id="listAllm" resultType="Kinds">
	select * from kinds
</select>

<insert id="addKindsm" parameterType="Kinds">
	insert into kinds(kinds_name) values(#{kinds_name})
</insert>
</mapper>

